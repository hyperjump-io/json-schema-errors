{
  "$schema": "../test-suite.schema.json",

  "description": "The const keyword",
  "tests": [
    {
      "description": "const",
      "compatibility": "6",
      "schema": {
        "const": 42
      },
      "instance": "foo",
      "errors": [
        {
          "messageId": "const-message",
          "messageParams": {
            "expected": "42"
          },
          "instanceLocation": "#",
          "schemaLocations": ["#/const"]
        }
      ]
    },
    {
      "description": "const pass",
      "compatibility": "6",
      "schema": {
        "const": 42
      },
      "instance": 42,
      "errors": []
    },
    {
      "description": "contradictory const - empty intersection",
      "compatibility": "6",
      "schema": {
        "allOf": [
          { "const": "a" },
          { "const": "b" }
        ]
      },
      "instance": "a",
      "errors": [
        {
          "messageId": "boolean-schema-message",
          "messageParams": {},
          "instanceLocation": "#",
          "schemaLocations": ["#/allOf/0/const", "#/allOf/1/const"]
        }
      ]
    }
  ]
}
